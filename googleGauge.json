{
	"id": "com.sap.sample.googleGauge",
	"version": "1.0.0",
	"name": "Google Gauge",
	"description": "Google Gauge",
	"newInstancePrefix": "googleGauge",
	"icon": "https://phomble.github.io/googleGauge/icon.png",
	"vendor": "I521939",
	"eula": "",
	"license": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-sample-google",
			"url": "https://phomble.github.io/googleGauge/googleGauge.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"label": {
			"type": "string",
			"description": "label",
			"default": "Label"
		},
		"value": {
			"type": "number",
			"description": "value",
			"default": 10
		},
		"redto": {
			"type": "number",
			"description": "value",
			"default": 0
		},
		"redfrom": {
			"type": "number",
			"description": "value",
			"default": 0
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the gauge value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "number",
					"description": "The new gauge value."
				}
			],
			"body": "this.value = newValue;"
		},
		"setRed": {
			"description": "Sets the gauge value.",
			
			"parameters": [
				{
					"name": "newrLowLim",
					"type": "number",
					"description": "Gauge Red start value"
				},
				{
					"name": "newrHiLim",
					"type": "number",
					"description": "Gauge Red end value"
				}
			],
			"body": "this.redto = newrHiLim; this.redfrom = newrLowLim;"
		},
		"getValue": {
			"returnType": "number",
			"description": "Returns the gauge value",
			"body": "return this.value;"
		},
		"setLabel": {
			"description": "Sets the label value.",
			"parameters": [
				{
					"name": "newLabel",
					"type": "string",
					"description": "The new label value."
				}
			],
			"body": "this.label = newLabel;"
		},
		"getLabel": {
			"returnType": "string",
			"description": "Returns the gauge value",
			"body": "return this.label;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Colored Box."
		}
	}
}
